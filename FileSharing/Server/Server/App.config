<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!--<appSettings>
    <add key="SavedLocation" value="..\\Files\"/>
    <add key="SavedLocationTemp" value="..\\FilesTemp\"/>
  </appSettings>-->
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NewBinding1" listenBacklog="10" maxBufferPoolSize="157286400"
            maxConnections="10" maxReceivedMessageSize="104857600" />
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="NewBehavior0">
          <serviceMetadata httpGetEnabled="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="NewBehavior0" name="FileSharing.Server.Service">
        <endpoint address="net.tcp://localhost:3021/FileSharingService/ep1"
            binding="netTcpBinding" bindingConfiguration="NewBinding1"
            name="ep1" contract="FileSharing.Server.IService" />
        <endpoint address="" binding="mexTcpBinding" bindingConfiguration=""
            name="mex" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:5210/FileSharingService" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>